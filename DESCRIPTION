Package: optimizeR
Title: Unified Framework for Numerical Optimizers
Version: 1.0.1.9000
Authors@R: c(
    person("Lennart", "Oelschl\u00e4ger", 
           email = "oelschlaeger.lennart@gmail.com", 
           role = c("aut", "cre"),
           comment = c(ORCID = "0000-0001-5421-9313")),
